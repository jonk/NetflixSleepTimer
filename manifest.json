{
    "name": "NetflixSleepTimer",
    "version": "0.0.1",
    "manifest_version": 2,
    "permissions": [
        "tabs",
        "declarativeContent"
    ],


    "page_action": {
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
        },
        "default_title": "Tool Tip Yo",
        "default_popup": "index.html"
    },

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    
    "devtools_page": "devtools/devtools.html"
}